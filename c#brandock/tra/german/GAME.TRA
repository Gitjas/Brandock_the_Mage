/* Brandock cre things */
@500 = ~Brandock~
@501 = ~[panisch] Ich... ich schaff's nicht mehr!~ [c#bran01]   //SAY MORALE
@502 = ~[fröhlich] Hier zu sein gefällt mir ausgesprochen gut!~ [c#bran27] //SAY HAPPY
@503 = ~[unglücklich] Das muss doch so nicht laufen!~ [c#bran21] //SAY UNHAPPY_ANNOYED
@504 = ~[generft] Langsam wird es mir zu bunt!~ [c#bran26] //SAY UNHAPPY_SERIOUS
@505 = ~[ärgerlich] Tut mir Leid, aber das war's!~ [c#bran02] //SAY UNHAPPY_BREAKING
@506 = ~[unsicher] Oje. Seid Ihr sicher?~ [c#bran08] //SAY LEADER
@507 = ~[müde] Ich bräuchte mal wieder eine Rast...~ [c#bran09] //SAY TIRED
@508 = ~[freundlich] Stehen wir noch länger hier?~ [c#bran10] //SAY BORED
@509 = ~[gerufen] Ich... ich kann auch gemein sein, wenn es sein muss!~ [c#bran03] //SAY BATTLE_CRY1
@510 = ~[geseufzt] Dann müssen wir wohl wieder!~ [c#bran05] //SAY BATTLE_CRY2
@511 = ~[gerufen] Macht Euch auf was gefasst!~ [c#bran06] //SAY BATTLE_CRY3
@512 = ~[geseufzt] Nicht schon wieder...~ [c#bran07] //SAY BATTLE_CRY4
@513 = ~[gerufen] Auf sie!~ [c#bran04] //SAY BATTLE_CRY5
@514 = ~[Schmerzgeräusch]~ [c#bran25] //SAY DAMAGE
@515 = ~[unter Schmerzen] Nicht... nicht so!~ [c#bran22] //SAY DYING
@516 = ~[gequält] Ich... ich bräuchte etwas Hilfe!~ [c#bran24] //SAY HURT
@517 = ~Ich liebe den Duft von Moos und Kiefernadeln.~ [c#bran28] //SAY AREA_FOREST
@518 = ~In der Stadt sind immer so viele Leute auf einem Haufen!~ [c#bran29] //SAY AREA_CITY
@519 = ~Irgendwie ist es hier unheimlich.~ [c#bran30] //SAY AREA_DUNGEON
@520 = ~Ich mag es, wenn die Sonne scheint! Außer direkt in mein Gesicht, natürlich.~ [c#bran31] //SAY AREA_DAY
@521 = ~Nachts ist es immer so... dunkel.~ [c#bran32] //SAY AREA_NIGHT
@522 = ~Ja?~ [c#bran11] //SAY SELECT_COMMON1
@523 = ~Was wollt Ihr?~ [c#bran12] //SAY SELECT_COMMON2
@524 = ~Was kann ich tun?~ [c#bran14] //SAY SELECT_COMMON3
@525 = ~[ergeben] In Ordnung.~ [c#bran15] //SAY SELECT_ACTION1
@526 = ~Das kann ich machen.~ [c#bran13] //SAY SELECT_ACTION2 
@527 = ~Ich werd's probieren.~ [c#bran16] //SAY SELECT_ACTION3 
@528 = ~Schon unterwegs.~ [c#bran17] //SAY SELECT_ACTION4
@529 = ~Gut.~ [c#bran18] //SAY SELECT_ACTION5
@530 = ~Alles klar!~ [c#bran19] //SAY SELECT_ACTION6
@531 = ~[bestätigend] Ja.~ [c#bran20] //SAY SELECT_ACTION7
@532 = ~Ach verdammt. Daran werde ich mich nie gewöhnen.~ [c#bran23] //SAY REACT_TO_DIE_GENERAL
@533 = ~Als Ihr BRANDOCK WASSERTIEF nach seiner Vergangenheit fragt, lächelt er entschuldigend und erzählt von seinem Plan, ein anerkannter Magier zu werden. "Über mich gibt es nicht viel Wissenswertes", fügt er hinzu. "Ich komme aus einer Familie aus Bauern und Tischlern. Kein weiteres bekanntes magisches Talent - außer bei einem entfernten Cousin meiner Mutter."~
@534 = ~M-hm?~ [c#bran33] //SAY SELECT_COMMON4
@535 = ~Ich höre?~ [c#bran34] //SAY SELECT_COMMON5
@536 = ~[als freundliche Aufforerung] Sagt, was Ihr wollt!~ [c#bran35] //SAY SELECT_COMMON6
@537 = ~Bei den Göttern! Erschreckt mich doch nicht so! Was wollt Ihr?~ [c#bran36] //SAY SELECT_RARE1
@538 = ~[triumphierend] Ja!!~ [c#bran37] //SAY CRITICAL_HIT
@539 = ~Verdammt, daneben!~ [c#bran38] //SAY CRITICAL_MISS
@540 = ~Meine Waffe scheint nichts zu bewirken.~ [c#bran39] //SAY TARGET_IMMUNE
@541 = ~[entschuldigend] Tut mir Leid, aber das passte wirklich nicht mehr in meine Tasche.~ [c#bran40] //SAY INVENTORY_FULL
@542 = ~Ach, Mist! Jetzt bin ich rausgekommen.~ [c#bran41] //SAY SPELL_DISRUPTED




/* scripts: DisplayStringHeads & Mapnotes */
/* BG1 */
@1000 = ~Der Tisch ist voller Utensilien und Material für alchemische Experimente.~
@1001 = ~Ähem!~
@1002 = ~Was für ein Kampf.~
@1003 = ~Hilfe! (Brandock scheint recht stark verletzt zu sein.)~
@1004 = ~Autsch!~
@1005 = ~Oh, was ist...~
@1006 = ~Also, das war wirklich unerwartet!~
@1007 = ~Magische Bücher! Viele Bücher! Werden helfen ...~
@1008 = ~Dies sind meine Privaträume. WIE seid Ihr hier reingekommen?! Ich sollte Euch beide einfach ohne Zögern...~
@1009 = ~Ihr seid nicht am Plündern. Ihr seid am Suchen?!...~
@1010 = ~Noch nie zuvor habe ich einen Halboger getroffen, der sich für meine magischen Bücher interessierte. Ich bitte um eine Erklärung. Wer seid Ihr?~
@1011 = ~Ich nicht Halboger. Ich Magier! Ich... Worte lassen Kopf wehtun!~
@1012 = ~Wer ich bin? Ich bin... ich meine, theoretisch gesprochen *war* ich früher eine Verkörperung eines anderen Lebewesens mit sehr viel begrenzterem intellektuellen Habitus, zumindest wenn man es in prinzipiellen Konditionen auszudrücken versucht...~
@1013 = ~Er nicht klar sprechen. Er dumm, mit meinem Kopf! Ich... intelligent, mit seinem dummem Kopf! Und plumpe Hände... brauche Hilfe!~
@1014 = ~Nungut, ich fasse das mal zusammen. Ich sehe einen Magier der sagt, nicht mehr der zu sein, der er war...~
@1015 = ~Und ich sehe einen Halboger, der angibt, der Magier zu sein.~
@1016 = ~Gerade als ich dachte, es würde ein ruhiger Abend werden. Kommt mit mir. Ich werde sehen, was ich für Euch tun kann.~
@1017 = ~Oje, ist das ein Gestank!~
@1018 = ~Hm, mal sehen...~
@1019 = ~Das war äußerst interessant! Danke, dass wir uns die Zeit nehmen konnten, <CHARNAME>.~
@1020 = ~Die Ecke der Höhle ist voller verdrecktem Zeug. Ihr verspürt absolut kein Verlangen, darin herum zu suchen.~
@1021 = ~Also, bin ich eigentlich der einzige, der findet, das das ein bisschen alarmierend klingt?~
@1022 = ~Firebead Elfenhaars Haus~
@1023 = ~Wir haben einen Sternenssphir! Bitte, lasst ihn uns gut verstauen, ich brauche ihn für die Buchrestauration.~
@1024 = ~Wir haben einen Regenerationstrank! Bitte, trinkt ihn nicht, ich brauche ihn für die Buchrestauration.~
@1025 = ~Wir haben Oktopustinte! Elfenhaar braucht sie für die Restauration meines Buches.~
